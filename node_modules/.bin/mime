#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="C:\Users\jay.mcdoniel\Documents\Node_Database_Tutorials\AlecTeleBot\node_modules\mime\node_modules;C:\Users\jay.mcdoniel\Documents\Node_Database_Tutorials\AlecTeleBot\node_modules;C:\Users\jay.mcdoniel\Documents\Node_Database_Tutorials\node_modules;C:\Users\jay.mcdoniel\Documents\node_modules;C:\Users\jay.mcdoniel\node_modules;C:\Users\node_modules;C:\node_modules" "$basedir/node"  "$basedir/../mime/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="C:\Users\jay.mcdoniel\Documents\Node_Database_Tutorials\AlecTeleBot\node_modules\mime\node_modules;C:\Users\jay.mcdoniel\Documents\Node_Database_Tutorials\AlecTeleBot\node_modules;C:\Users\jay.mcdoniel\Documents\Node_Database_Tutorials\node_modules;C:\Users\jay.mcdoniel\Documents\node_modules;C:\Users\jay.mcdoniel\node_modules;C:\Users\node_modules;C:\node_modules" node  "$basedir/../mime/cli.js" "$@"
  ret=$?
fi
exit $ret
